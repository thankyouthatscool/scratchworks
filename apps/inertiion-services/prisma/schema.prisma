// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum EventType {
  locationUpdate
}

model Event {
  id          String    @id @default(uuid())
  target      String
  type        EventType
  description String    @default("")
}

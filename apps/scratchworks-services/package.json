{
  "name": "@scratchworks/scratchworks-services",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "dev": "dotenv -e .env nodemon",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "dev": "dotenv -e .env nodemon",
    "image:build": "docker build . -t ozahnitko/scratchworks-services:dev-latest",
    "image:push": "docker push ozahnitko/scratchworks-services:dev-latest",
    "image:run": "docker run -d --env-file .env --env NODE_ENV=prod --name scratchworks-services ozahnitko/scratchworks-services:dev-latest",
    "start:prod": "node ./build/index.js"
  },
  "dependencies": {
    "@trpc/server": "^10.9.0",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
